import java.util.Scanner;


public class Main {

	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		String[] words = new String[7];
		for (int i = 0; i < 7; i ++){
		     words[i] = in.nextLine();
		}
		generate(words);
	}
	
	
	public static void generate(String[] words){
		for (int x=6; x >= 0; x--){
			for (int y=6; y > x; y--){
			    System.out.println(words[x]+ '-' + words[y]);
			}
		}
	}

}

