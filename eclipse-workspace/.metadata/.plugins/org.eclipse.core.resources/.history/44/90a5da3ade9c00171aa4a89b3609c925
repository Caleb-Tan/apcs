import java.util.Scanner;


public class Main {

	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		String[] words = new String[7];
		for (int i = 0; i < 6; i ++)
		{
		     words[i] = in.nextLine();
		}
		generate(words);
	}
	
	
	public static void generate(String[] words){
		for (int i = 0; i < 6; i ++)
		{
		     System.out.println(words);
		}
	}

}

