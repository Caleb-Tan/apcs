package flag;
import java.awt.*;
import javax.swing.JPanel;


public class Flag extends JPanel{
	
	
	 public void paint(Graphics g){
		 paintRectangle(g);
			 
	 }
	 
	 public void paintRectangle(int g) {
			g.setColor(Color.red);
			g.fillRect(0, g, 950, 100);
			g.setColor(Color.white);
			g.fillRect(0, g, 950, 100);
			paintRectangle(g);
	 }
}
